# ============================================================================
# TMarks 缓存配置示例
# ============================================================================

# 缓存级别 (0-3)
# 0 = 无缓存 (最低成本, ~$5/月)
# 1 = 最小缓存 (推荐默认, ~$8/月) ⭐
# 2 = 标准缓存 (推荐生产, ~$12/月)
# 3 = 激进缓存 (高性能, ~$20-30/月)
CACHE_LEVEL=1

# 是否启用 KV 缓存
# true = 启用, false = 禁用 (等同于 CACHE_LEVEL=0)
ENABLE_KV_CACHE=true

# ============================================================================
# 高级配置 (可选，覆盖默认值)
# ============================================================================

# 自定义 TTL (秒)
# CACHE_TTL_DEFAULT_LIST=600       # 默认列表缓存时间 (10分钟)
# CACHE_TTL_TAG_FILTER=600         # 标签筛选缓存时间 (10分钟)
# CACHE_TTL_SEARCH=300             # 搜索结果缓存时间 (5分钟)
# CACHE_TTL_PUBLIC_SHARE=600       # 公开分享缓存时间 (10分钟)

# 内存缓存配置
# ENABLE_MEMORY_CACHE=true         # 是否启用内存缓存 (Level 2+)
# MEMORY_CACHE_MAX_AGE=60          # 内存缓存最大时间 (60秒)

# 调试模式
# CACHE_DEBUG=false                # 是否启用缓存调试日志

# ============================================================================
# 推荐配置
# ============================================================================

# 个人使用 (<100 书签)
# CACHE_LEVEL=0 或 1

# 小型团队 (<1000 用户)
# CACHE_LEVEL=1 (默认)

# 生产环境 (1000-10000 用户)
# CACHE_LEVEL=2

# 大型应用 (>10000 用户)
# CACHE_LEVEL=3

# ============================================================================
# 成本说明
# ============================================================================

# Level 0: ~$5/月
#   - 仅速率限制
#   - 响应时间: 100-300ms
#   - 适合个人使用

# Level 1: ~$8/月 ⭐ (推荐默认)
#   - 缓存默认列表和公开分享
#   - 响应时间: 50-100ms
#   - 缓存命中率: 60-70%
#   - 批量导入不写缓存 (零成本)

# Level 2: ~$12/月 (推荐生产)
#   - 缓存标签筛选
#   - 启用内存缓存
#   - 响应时间: 30-50ms
#   - 缓存命中率: 80-85%
#   - 批量导入不写缓存 (零成本)

# Level 3: ~$20-30/月
#   - 缓存所有查询
#   - 响应时间: 20-30ms
#   - 缓存命中率: 90-95%
#   - 批量导入异步写缓存 (成本可忽略)
